{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "singleAttributePerLine": false,
  "plugins": ["prettier-plugin-vue", "@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^primevue.*$",
    "",
    "^@prime.*$",
    "",
    "^@primevue.*$",
    "",
    "^vue.*$",
    "^pinia.*$",
    "",
    "^axios$",
    "",
    "^@/components/.*$",
    "",
    "^@/composables/.*$",
    "",
    "^@/router/.*$",
    "",
    "^@/services/.*$",
    "",
    "^@/types$",
    "",
    "^@/views$",
    "",
    "^@/.*$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrderCaseSensitive": false
}
